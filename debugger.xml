<application>
  <component name="XDebuggerSettings">
    <data-views>
      <option name="autoExpressions" value="false" />
      <option name="valueLookupDelay" value="50" />
      <show-values-inline>false</show-values-inline>
    </data-views>
    <general>
      <evaluation-dialog-mode>CODE_FRAGMENT</evaluation-dialog-mode>
      <run-to-cursor-gesture>false</run-to-cursor-gesture>
    </general>
    <debuggers>
      <debugger id="dotnet_debugger">
        <configuration>
          <option name="automaticallyRefreshWatches" value="true" />
          <option name="editAndContinueEnabled" value="false" />
          <option name="evaluationTimeoutMs" value="15000" />
          <option name="needNotifyWhenStoppedInExternalCode" value="false" />
          <option name="showReturnValues" value="false" />
          <option name="showRunToPanel" value="false" />
          <option name="smartStepIntoByDefault" value="false" />
        </configuration>
      </debugger>
      <debugger id="javascript">
        <configuration>
          <custom-object-presentation />
        </configuration>
      </debugger>
      <debugger id="oracle">
        <configuration>
          <option name="steppingPauseAtBegin" value="true" />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>